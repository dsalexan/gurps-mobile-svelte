				#newitem( AD:Extraordinary Luck, 15, displaycost(+15), page(B51), cat(Mundane, Mental), existing(AD:Luck) ) #codes(upto 2, downto 2)_
			)_
		),_
	select2(comment(Kid),_
		text(Disadvantages: Select -5 points from the following:),_
		tagwith(Kid(yes)),_
		pointswanted(-5),_
		itemswanted(atleast 1),_
		list(_
				#newitem(DI:Gullibility, -10, displaycost(-5*), gives(-3 To SK:Merchant), mods(Self-Control), page(B137), cat(Mundane, Mental), initmods( You resist on a roll of 15 or less (quite often), *0.5, shortname(15 or less), group(Self-Control), page(B121) ), taboo(SK:Detect Lies)),_
