	description(Any delver might return to school and become a scholar.),_
	page(DF4:11),_
	adds(_
			AD:Eidetic Memory=1,_
			SK:Cartography=1pts,_
			SK:Speed-Reading=1pts_
	),_
	creates(_
		{AD:Modular Abilities (Book-Learned Wisdom), 0, displaycost(5+2/point), mods(Book-Learned Wisdom, Modular Ability), page(B71), cat(Exotic Mental, Exotic Physical, Exotic, Mental, Physical)} _
				with {Skill 1, 5+2/point,formula(5+(me::level*2)),forceformula(yes),group(Book-Learned Wisdom),page(B71),mods(Modular Abilities),level(2)},_
		{AD:Wild Talent, 20/40, mods(Wild Talent), page(B99), cat(Supernatural, Mental)}=1 _
				with { No Advantage Requirement, +50%, group(Wild Talent), page(DF4:8) }_
	),_
	select1(_
		text("Pick a Hidden Lore specialties. Do not select a specialty that you already have."),_
		tagwith(Scholar(yes)),_
		itemswanted(3),_
		pointswanted(3),_
		list(_
			SK:Hidden Lore (Demon Lore) #codes(upto 1pts),_
			SK:Hidden Lore (Elemental Lore) #codes(upto 1pts),_
			SK:Hidden Lore (Faerie Lore) #codes(upto 1pts),_
			SK:Hidden Lore (Lost Civilizations) #codes(upto 1pts),_
			SK:Hidden Lore (Magic Items) #codes(upto 1pts),_
			SK:Hidden Lore (Magical Writings) #codes(upto 1pts),_
			SK:Hidden Lore (Spirit Lore) #codes(upto 1pts),_
			SK:Hidden Lore (Undead Lore) #codes(upto 1pts)_
		)_
	),_
	select2(_
		text("Take Research skill, or add Boost Research Skill to add [1] point to an existing one."),_
		tagwith(Scholar(yes)),_
		itemswanted(1),_
		pointswanted(1),_
		multitype(yes),_
		list(_
			SK:Research #codes(upto 1pts),_
			#newitem(AD:_Increase Research Skill, 1, gives(+1pts to "SK:Research"), taboo("SK:Research"), needs("SK:Research"), existing(SK:Research) )_
		)_
	)

