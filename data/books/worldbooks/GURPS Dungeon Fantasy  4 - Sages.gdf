<Character Templates - Dungeon Fantasy - Mixed Professions - Wizard, Character Templates - Dungeon Fantasy - Mixed Professions, Character Templates>
Wizard-Scholar (Dungeon Fantasy), displaycost(+50), cost(0),_
	description(Any delver might return to school and become a scholar.),_
	page(DF4:11),_
	adds(_
			AD:Eidetic Memory=1_
	),_
	creates(_
		{AD:Modular Abilities (Book-Learned Wisdom), 0, displaycost(5+2/point), mods(Book-Learned Wisdom, Modular Ability), page(B71), cat(Exotic Mental, Exotic Physical, Exotic, Mental, Physical)} _
				with {Skill 1, 5+2/point,formula(5+(me::level*2)),forceformula(yes),group(Book-Learned Wisdom),page(B71),mods(Modular Abilities),level(2)},_
		{AD:Wild Talent, 20/40, mods(Wild Talent), page(B99), cat(Supernatural, Mental)}=1 _
				with { No Advantage Requirement, +50%, group(Wild Talent), page(DF4:8) }_
	),_
	select1(_
		text(Please add 2 points to one of your Hidden Lore skill:),_
		pointswanted(2),_
		tagwith(Scholar(yes)),_
		itemswanted(1),_
		multitype(yes),_
		list(_
			#BuildCharItemList(Skills where Name includes "Hidden Lore", template(#newitem(AD:_Increase %ListItem%, existing(SK:%ListItem%)) #codes(upto %points+2pts, downto %points+2pts) ))_
		)_
	),_
	select2(_
		text("Pick three Hidden Lore specialties. Do not select a specialty that you already have."),_
		tagwith(Scholar(yes)),_
		itemswanted(3),_
		pointswanted(3),_
		list(_
			SK:Hidden Lore (Demon Lore) #codes(upto 1pts),_
			SK:Hidden Lore (Elemental Lore) #codes(upto 1pts),_
			SK:Hidden Lore (Faerie Lore) #codes(upto 1pts),_
			SK:Hidden Lore (Lost Civilizations) #codes(upto 1pts),_
			SK:Hidden Lore (Magic Items) #codes(upto 1pts),_
			SK:Hidden Lore (Magical Writings) #codes(upto 1pts),_
			SK:Hidden Lore (Spirit Lore) #codes(upto 1pts),_
			SK:Hidden Lore (Undead Lore) #codes(upto 1pts)_
		)_
	),_
	select3(_
		text("Add Cartography if you don't already have it, otherwise select Boost Cartography Skill and add it to the skill later."),_
		tagwith(Scholar(yes)),_
		itemswanted(1),_
		pointswanted(1),_
		multitype(yes),_
		list(_
			SK:Cartography #codes(upto 1pts),_
			#newitem(AD:_Increase Cartography Skill, 1, gives(+1pts to "SK:Cartography"), taboo("SK:Cartography"), needs("SK:Cartography"), existing(SK:Cartography) )_
		)_
	),_
	select4(_
		text("Add [2] points to your primary Hidden Lore specialty."),_
		tagwith(Scholar(yes)),_
		itemswanted(1),_
		pointswanted(2),_
		list(_
			#newitem(AD:_Increase Demon Lore Skill, 2, gives(+2pts to "SK:Hidden Lore (Demon Lore)"), taboo("SK:Hidden Lore (Demon Lore)"), needs("SK:Hidden Lore (Demon Lore)"), existing(SK:Hidden Lore (Demon Lore)) ),_
			#newitem(AD:_Increase Magic Items Lore Skill, 2, gives(+2pts to "SK:Hidden Lore (Magic Items Lore)"), taboo("SK:SK:Hidden Lore (Magic Items Lore)"), needs("SK:Hidden Lore (Magic Items Lore)"), existing(SK:Hidden Lore (Magic Items Lore)) ),_
			#newitem(AD:_Increase Magical Writings Lore Skill, 2, gives(+2pts to "SK:Hidden Lore (Magical Writings Lore)"), taboo("SK:Hidden Lore (Magical Writings Lore)"), needs("SK:Hidden Lore (Magical Writings Lore)"), existing(SK:Hidden Lore (Magical Writings Lore)) ),_
			#newitem(AD:_Increase Spirit Lore Skill, 2, gives(+2pts to "SK:Hidden Lore (Spirit Lore)"), taboo("SK:Hidden Lore (Spirit Lore)"), needs("SK:Hidden Lore (Spirit Lore)"), existing(SK:Hidden Lore (Spirit Lore)) )_
		)_
	)

