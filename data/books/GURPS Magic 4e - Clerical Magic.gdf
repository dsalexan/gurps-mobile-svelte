			name(type),_
			title(New Spell: Choose a Skill Type),_
			text("Choose the type of spell from the list below:"),_
			picksallowed(1),_
			method(bynumber),_
			list(_
				"IQ/H",_
				"IQ/VH" _
			)_
			),_
