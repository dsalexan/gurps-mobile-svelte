[ADVANTAGES]
#DeleteByTag Advantages, Text IgnoreEmpty, cat HasInList Supernatural
#DeleteByTag Advantages, Text IgnoreEmpty, cat HasInList Exotic
#DeleteByTag Advantages, Text IgnoreEmpty, cat HasInList Innate Attack

[DISADVANTAGES]
#DeleteByTag Disadvantages, Text IgnoreEmpty, cat HasInList Supernatural
#DeleteByTag Disadvantages, Text IgnoreEmpty, cat HasInList Exotic

[TEMPLATES]
#DeleteByTag Templates, Text IgnoreEmpty, cat HasInList Racial Templates
#DeleteByTag Templates, Text IgnoreEmpty, cat HasInList Meta-Traits

